{
  "name": "blackroad-dev",
  "image": "mcr.microsoft.com/devcontainers/python:3.11-node",
  "features": {
    "ghcr.io/devcontainers/features/node:1": {},
    "ghcr.io/devcontainers/features/git:1": {}
  },
  "postCreateCommand": "pip install -r requirements.txt && npm i -g gitleaks",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "esbenp.prettier-vscode"
      ]
    }
  }
}
