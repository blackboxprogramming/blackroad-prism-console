# Postgres
PG_URL=postgres://user:pass@host:5432/app
PG_SOURCE_QUERY=select id, occurred_at, user_id, event_name, payload from app.events where occurred_at > $1 order by occurred_at asc
WATERMARK_SOURCE=pg.app.events

# S3 landing
S3_BUCKET=br-data-landing
S3_PREFIX=dev/app/events

# Snowflake (Path A: COPY INTO)
SNOWFLAKE_ACCOUNT=abc-xy123
SNOWFLAKE_USER=BR_EL
SNOWFLAKE_PASSWORD=*** # or SNOWFLAKE_PRIVATE_KEY + passphrase
SNOWFLAKE_WAREHOUSE=BR_WH_DEV
SNOWFLAKE_DATABASE=BR_RAW
SNOWFLAKE_SCHEMA=APP
SNOWFLAKE_STAGE=BR_LANDING_STAGE    # created via SQL below
SNOWFLAKE_TARGET_TABLE=EVENTS

# Batch sizing
BATCH_ROWS=10000
