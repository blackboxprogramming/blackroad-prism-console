{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Integrator Drift Report",
  "type": "object",
  "required": ["source", "mapper", "drift"],
  "properties": {
    "source": {
      "type": "string",
      "minLength": 1
    },
    "mapper": {
      "type": "string",
      "minLength": 1
    },
    "drift": {
      "type": "boolean"
    },
    "missing_fields": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "unexpected_fields": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "notes": {
      "type": "string"
    }
  },
  "additionalProperties": false
}
