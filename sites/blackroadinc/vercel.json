{
  "redirects": [
    { "source": "/:path*", "destination": "https://blackroad.io/:path*", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Strict-Transport-Security", "value": "max-age=15552000; includeSubDomains; preload" }
      ]
    }
  ]
}
