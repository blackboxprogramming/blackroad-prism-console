{
  "redirects": [
    { "source": "/blackroadinc", "destination": "https://blackroadinc.io", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=15552000; includeSubDomains; preload"
        }
      ]
    }
  ]
}
